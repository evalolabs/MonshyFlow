{
  "id": "kitemaker",
  "name": "Kitemaker (API)",
  "icon": "ðŸ“‹",
  "description": "Product management platform with GraphQL API",
  "baseUrl": "https://toil.kitemaker.co/developers",
  "color": {
    "bg": "from-indigo-100 to-violet-100",
    "border": "border-indigo-500",
    "icon": "text-indigo-700",
    "handle": "bg-indigo-600"
  },
  "authentication": {
    "type": "bearer",
    "headerName": "Authorization",
    "headerFormat": "Bearer {apiKey}",
    "secretKey": "KITEMAKER_API_TOKEN",
    "note": "Uses GraphQL instead of REST API. All requests are POST to /graphql endpoint with GraphQL query/mutation in the body."
  },
  "endpoints": [
    {
      "id": "graphql-query",
      "name": "GraphQL Query",
      "description": "Execute a GraphQL query or mutation",
      "method": "POST",
      "path": "/graphql",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string",
            "required": true,
            "description": "GraphQL query or mutation string"
          },
          "variables": {
            "type": "object",
            "description": "GraphQL variables",
            "additionalProperties": true
          },
          "operationName": {
            "type": "string",
            "description": "Operation name (for queries with multiple operations)"
          }
        },
        "required": ["query"]
      },
      "urlTemplate": "https://toil.kitemaker.co/developers/graphql"
    },
    {
      "id": "create-work-item",
      "name": "Create Work Item",
      "description": "Create a new work item (GraphQL mutation: createWorkItem)",
      "method": "POST",
      "path": "/graphql",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string",
            "default": "mutation CreateWorkItem($input: CreateWorkItemInput!) { createWorkItem(input: $input) { id title } }",
            "description": "GraphQL mutation for createWorkItem"
          },
          "variables": {
            "type": "object",
            "properties": {
              "input": {
                "type": "object",
                "description": "CreateWorkItemInput",
                "additionalProperties": true
              }
            }
          }
        },
        "required": ["query"]
      },
      "urlTemplate": "https://toil.kitemaker.co/developers/graphql"
    },
    {
