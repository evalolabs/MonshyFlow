          },
          "phone": {
            "type": "string",
            "required": true,
            "description": "Recipient phone number"
          },
          "notes": { "type": "string" },
          "skipSMSNotifications": { "type": "boolean", "default": false },
          "skipPhoneNumberValidation": { "type": "boolean", "default": false }
        },
        "required": ["name", "phone"]
      },
      "urlTemplate": "https://onfleet.com/api/v2/recipients"
    },
    {
      "id": "get-recipient",
      "name": "Get Recipient",
