    {
      "id": "create-coupon",
      "name": "Create Coupon",
      "description": "Create a new coupon",
      "method": "POST",
      "path": "/coupons",
      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "code": { "type": "string", "required": true, "description": "Coupon code" },
          "discount_type": { "type": "string", "enum": ["fixed_cart", "fixed_product", "percent"], "required": true, "description": "Discount type" },
          "amount": { "type": "string", "required": true, "description": "Discount amount" },
          "individual_use": { "type": "boolean", "default": false, "description": "Individual use only" },
          "exclude_sale_items": { "type": "boolean", "default": false, "description": "Exclude sale items" },
          "minimum_amount": { "type": "string", "description": "Minimum order amount" },
          "maximum_amount": { "type": "string", "description": "Maximum order amount" },
          "usage_limit": { "type": "number", "description": "Usage limit per coupon" },
          "usage_limit_per_user": { "type": "number", "description": "Usage limit per user" },
          "limit_usage_to_x_items": { "type": "number", "description": "Limit to X items" },
          "free_shipping": { "type": "boolean", "default": false, "description": "Free shipping" },
          "product_ids": {
            "type": "array",
            "items": { "type": "number" },
            "description": "Product IDs (if applicable)"
          },
          "excluded_product_ids": {
            "type": "array",
            "items": { "type": "number" },
            "description": "Excluded product IDs"
          },
          "product_categories": {
            "type": "array",
            "items": { "type": "number" },
            "description": "Product category IDs"
          },
          "excluded_product_categories": {
            "type": "array",
            "items": { "type": "number" },
            "description": "Excluded category IDs"
          },
          "expiry_date": { "type": "string", "format": "date-time", "description": "Expiry date" }
        },
        "required": ["code", "discount_type", "amount"]
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/coupons"
    },
    {
      "id": "get-coupon",
      "name": "Get Coupon",
      "description": "Get coupon details by ID",
      "method": "GET",
      "path": "/coupons/{id}",
      "headers": {},
