{
  "id": "woocommerce",
  "name": "WooCommerce (API)",
  "icon": "ðŸ›’",
  "logoUrl": "https://cdn.simpleicons.org/woocommerce/96588A",
  "description": "E-commerce platform for WordPress",
  "baseUrl": "https://{store}.com/wp-json/wc/v3",
  "color": {
    "bg": "from-purple-100 to-violet-100",
    "border": "border-purple-500",
    "icon": "text-purple-700",
    "handle": "bg-purple-600"
  },
  "authentication": {
    "type": "basic",
    "headerName": "Authorization",
    "headerFormat": "Basic {base64(consumerKey:consumerSecret)}",
    "secretKey": "WOocommerce_CONSUMER_SECRET",
    "emailSecretKey": "WOocommerce_CONSUMER_KEY"
  },
  "endpoints": [
    {
      "id": "create-product",
      "name": "Create Product",
      "description": "Create a new product",
      "method": "POST",
      "path": "/products",
      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "name": { "type": "string", "required": true, "description": "Product name" },
          "type": { "type": "string", "enum": ["simple", "grouped", "external", "variable"], "default": "simple", "description": "Product type" },
          "regular_price": { "type": "string", "description": "Regular price" },
          "sale_price": { "type": "string", "description": "Sale price" },
          "description": { "type": "string", "description": "Product description (HTML)" },
          "short_description": { "type": "string", "description": "Product short description" },
          "sku": { "type": "string", "description": "SKU" },
          "manage_stock": { "type": "boolean", "default": false, "description": "Manage stock" },
          "stock_quantity": { "type": "number", "description": "Stock quantity" },
          "stock_status": { "type": "string", "enum": ["instock", "outofstock", "onbackorder"], "default": "instock" },
          "weight": { "type": "string", "description": "Product weight" },
          "dimensions": {
            "type": "object",
            "properties": {
              "length": { "type": "string" },
              "width": { "type": "string" },
              "height": { "type": "string" }
            }
          },
          "categories": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": { "type": "number" },
                "name": { "type": "string" }
              }
            }
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": { "type": "number" },
                "name": { "type": "string" }
              }
            }
          },
          "images": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "src": { "type": "string", "format": "uri", "required": true },
                "name": { "type": "string" },
                "alt": { "type": "string" }
              },
              "required": ["src"]
            }
          },
          "status": { "type": "string", "enum": ["draft", "pending", "private", "publish"], "default": "publish" },
          "featured": { "type": "boolean", "default": false },
          "catalog_visibility": { "type": "string", "enum": ["visible", "catalog", "search", "hidden"], "default": "visible" }
        },
        "required": ["name"]
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/products"
    },
    {
      "id": "get-product",
      "name": "Get Product",
      "description": "Get product details by ID",
      "method": "GET",
      "path": "/products/{id}",
      "headers": {},
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/products/{id}"
    },
    {
      "id": "list-products",
      "name": "List Products",
      "description": "List all products",
      "method": "GET",
      "path": "/products",
      "headers": {},
      "queryParams": {
        "page": "1",
        "per_page": "10",
        "search": "{searchTerm}",
        "status": "publish",
        "category": "{categoryId}",
        "tag": "{tagId}",
        "featured": "true",
        "orderby": "date",
        "order": "desc"
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/products"
    },
    {
      "id": "update-product",
      "name": "Update Product",
      "description": "Update an existing product",
      "method": "PUT",
      "path": "/products/{id}",
      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "name": { "type": "string", "description": "Product name" },
          "regular_price": { "type": "string", "description": "Regular price" },
          "sale_price": { "type": "string", "description": "Sale price" },
          "description": { "type": "string", "description": "Product description" },
          "stock_quantity": { "type": "number", "description": "Stock quantity" },
          "status": { "type": "string", "enum": ["draft", "pending", "private", "publish"] },
          "featured": { "type": "boolean" }
        }
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/products/{id}"
    },
    {
      "id": "delete-product",
      "name": "Delete Product",
      "description": "Delete a product",
      "method": "DELETE",
      "path": "/products/{id}",
      "headers": {},
      "queryParams": {
        "force": "true"
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/products/{id}?force=true"
    },
    {
      "id": "create-order",
      "name": "Create Order",
      "description": "Create a new order",
      "method": "POST",
      "path": "/orders",
      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "payment_method": { "type": "string", "required": true, "description": "Payment method" },
          "payment_method_title": { "type": "string", "description": "Payment method title" },
          "set_paid": { "type": "boolean", "default": false, "description": "Set order as paid" },
          "billing": {
            "type": "object",
            "properties": {
              "first_name": { "type": "string", "required": true },
              "last_name": { "type": "string", "required": true },
              "company": { "type": "string" },
              "address_1": { "type": "string", "required": true },
              "address_2": { "type": "string" },
              "city": { "type": "string", "required": true },
              "state": { "type": "string", "required": true },
              "postcode": { "type": "string", "required": true },
              "country": { "type": "string", "required": true },
              "email": { "type": "string", "format": "email", "required": true },
              "phone": { "type": "string" }
            },
            "required": true
          },
          "shipping": {
            "type": "object",
            "properties": {
              "first_name": { "type": "string", "required": true },
              "last_name": { "type": "string", "required": true },
              "company": { "type": "string" },
              "address_1": { "type": "string", "required": true },
              "address_2": { "type": "string" },
              "city": { "type": "string", "required": true },
              "state": { "type": "string", "required": true },
              "postcode": { "type": "string", "required": true },
              "country": { "type": "string", "required": true }
            },
            "required": true
          },
          "line_items": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "product_id": { "type": "number", "required": true },
                "quantity": { "type": "number", "required": true },
                "variation_id": { "type": "number" },
                "subtotal": { "type": "string" },
                "total": { "type": "string" }
              },
              "required": ["product_id", "quantity"]
            },
            "required": true
          },
          "shipping_lines": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "method_id": { "type": "string", "required": true },
                "method_title": { "type": "string", "required": true },
                "total": { "type": "string", "required": true }
              },
              "required": ["method_id", "method_title", "total"]
            }
          },
          "fee_lines": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": { "type": "string", "required": true },
                "total": { "type": "string", "required": true },
                "taxable": { "type": "boolean", "default": false }
              },
              "required": ["name", "total"]
            }
          },
          "coupon_lines": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "code": { "type": "string", "required": true }
              },
              "required": ["code"]
            }
          },
          "status": { "type": "string", "enum": ["pending", "processing", "on-hold", "completed", "cancelled", "refunded", "failed"], "default": "pending" },
          "customer_id": { "type": "number", "description": "Customer ID" },
          "customer_note": { "type": "string", "description": "Customer note" },
          "meta_data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "key": { "type": "string", "required": true },
                "value": { "type": ["string", "number", "boolean", "object"], "required": true }
              },
              "required": ["key", "value"]
            }
          }
        },
        "required": ["payment_method", "billing", "shipping", "line_items"]
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/orders"
    },
    {
      "id": "get-order",
      "name": "Get Order",
      "description": "Get order details by ID",
      "method": "GET",
      "path": "/orders/{id}",
      "headers": {},
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/orders/{id}"
    },
    {
      "id": "list-orders",
      "name": "List Orders",
      "description": "List all orders",
      "method": "GET",
      "path": "/orders",
      "headers": {},
      "queryParams": {
        "page": "1",
        "per_page": "10",
        "status": "any",
        "customer": "{customerId}",
        "product": "{productId}",
        "orderby": "date",
        "order": "desc",
        "after": "{date}",
        "before": "{date}"
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/orders"
    },
    {
      "id": "update-order",
      "name": "Update Order",
      "description": "Update an existing order",
      "method": "PUT",
      "path": "/orders/{id}",
      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "status": { "type": "string", "enum": ["pending", "processing", "on-hold", "completed", "cancelled", "refunded", "failed"] },
          "customer_note": { "type": "string" },
          "billing": {
            "type": "object",
            "properties": {
              "first_name": { "type": "string" },
              "last_name": { "type": "string" },
              "email": { "type": "string", "format": "email" },
              "phone": { "type": "string" }
            }
          },
          "shipping": {
            "type": "object",
            "properties": {
              "first_name": { "type": "string" },
              "last_name": { "type": "string" }
            }
          }
        }
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/orders/{id}"
    },
    {
      "id": "delete-order",
      "name": "Delete Order",
      "description": "Delete an order",
      "method": "DELETE",
      "path": "/orders/{id}",
      "headers": {},
      "queryParams": {
        "force": "true"
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/orders/{id}?force=true"
    },
    {
