      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "amount": { "type": "string", "description": "Discount amount" },
          "usage_limit": { "type": "number", "description": "Usage limit" },
          "expiry_date": { "type": "string", "format": "date-time", "description": "Expiry date" }
        }
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/coupons/{id}"
    },
    {
      "id": "delete-coupon",
