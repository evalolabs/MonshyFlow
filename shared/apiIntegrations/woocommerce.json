          "username": { "type": "string", "description": "Username (auto-generated from email if not provided)" },
          "password": { "type": "string", "description": "Password" },
          "billing": {
            "type": "object",
            "properties": {
              "first_name": { "type": "string" },
              "last_name": { "type": "string" },
              "company": { "type": "string" },
              "address_1": { "type": "string" },
              "address_2": { "type": "string" },
              "city": { "type": "string" },
              "state": { "type": "string" },
              "postcode": { "type": "string" },
              "country": { "type": "string" },
              "email": { "type": "string", "format": "email" },
              "phone": { "type": "string" }
            }
          },
          "shipping": {
            "type": "object",
            "properties": {
              "first_name": { "type": "string" },
              "last_name": { "type": "string" },
              "company": { "type": "string" },
              "address_1": { "type": "string" },
              "address_2": { "type": "string" },
              "city": { "type": "string" },
              "state": { "type": "string" },
              "postcode": { "type": "string" },
              "country": { "type": "string" }
            }
          },
          "meta_data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "key": { "type": "string", "required": true },
                "value": { "type": ["string", "number", "boolean", "object"], "required": true }
              }
            }
          }
        },
        "required": ["email"]
      },
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/customers"
    },
    {
      "id": "get-customer",
      "name": "Get Customer",
      "description": "Get customer details by ID",
      "method": "GET",
      "path": "/customers/{id}",
      "headers": {},
      "urlTemplate": "https://{store}.com/wp-json/wc/v3/customers/{id}"
    },
    {
      "id": "list-customers",
      "name": "List Customers",
