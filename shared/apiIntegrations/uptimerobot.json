{
  "id": "uptimerobot",
  "name": "UptimeRobot (API)",
  "icon": "ðŸ“Š",
  "description": "Website monitoring and uptime tracking",
  "baseUrl": "https://api.uptimerobot.com/v2",
  "color": {
    "bg": "from-green-100 to-cyan-100",
    "border": "border-green-500",
    "icon": "text-green-700",
    "handle": "bg-green-600"
  },
  "authentication": {
    "type": "apiKey",
    "headerName": "Content-Type",
    "headerFormat": "application/x-www-form-urlencoded",
    "secretKey": "UPTIMEROBOT_API_KEY",
    "note": "UptimeRobot API uses POST requests for all operations, including GET operations. API key is sent as form data."
  },
  "endpoints": [
    {
      "id": "get-account-details",
      "name": "Get Account Details",
      "description": "Get account information",
      "method": "POST",
      "path": "/getAccountDetails",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "api_key": { "type": "string", "required": true, "description": "API key" }
        },
        "required": ["api_key"]
      },
      "urlTemplate": "https://api.uptimerobot.com/v2/getAccountDetails"
    },
    {
      "id": "create-monitor",
      "name": "Create Monitor",
      "description": "Create a new monitor",
      "method": "POST",
      "path": "/newMonitor",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "api_key": { "type": "string", "required": true, "description": "API key" },
          "friendly_name": { "type": "string", "required": true, "description": "Monitor friendly name" },
          "url": { "type": "string", "format": "uri", "required": true, "description": "URL to monitor" },
          "type": { "type": "integer", "required": true, "enum": [1, 2, 3, 4], "description": "Monitor type: 1=HTTP(s), 2=Keyword, 3=Ping, 4=Port" },
          "interval": { "type": "integer", "default": 300, "description": "Monitoring interval in seconds" },
          "keyword_type": { "type": "integer", "enum": [1, 2], "description": "Keyword type: 1=exists, 2=not exists (for type 2)" },
          "keyword_value": { "type": "string", "description": "Keyword value (for type 2)" },
          "http_username": { "type": "string", "description": "HTTP basic auth username" },
          "http_password": { "type": "string", "description": "HTTP basic auth password" },
          "alert_contacts": { "type": "string", "description": "Comma-separated alert contact IDs" },
          "mwindows": { "type": "string", "description": "Comma-separated maintenance window IDs" }
        },
        "required": ["api_key", "friendly_name", "url", "type"]
      },
      "urlTemplate": "https://api.uptimerobot.com/v2/newMonitor"
    },
    {
      "id": "get-monitors",
      "name": "Get Monitors",
      "description": "Get all monitors",
      "method": "POST",
      "path": "/getMonitors",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "api_key": { "type": "string", "required": true, "description": "API key" },
          "monitors": { "type": "string", "description": "Comma-separated monitor IDs" },
          "types": { "type": "string", "description": "Comma-separated monitor types" },
          "statuses": { "type": "string", "description": "Comma-separated statuses: 0=paused, 1=not checked yet, 2=up, 8=seems down, 9=down" },
          "custom_uptime_ratios": { "type": "string", "description": "Comma-separated custom uptime ratios" },
          "custom_uptime_ranges": { "type": "string", "description": "Comma-separated custom uptime ranges" },
          "logs": { "type": "integer", "enum": [0, 1], "default": 0, "description": "Include logs: 0=no, 1=yes" },
          "log_types": { "type": "string", "description": "Comma-separated log types" },
          "log_limit": { "type": "integer", "default": 1, "description": "Number of logs to return" },
          "response_times": { "type": "integer", "enum": [0, 1], "default": 0, "description": "Include response times: 0=no, 1=yes" },
          "response_times_limit": { "type": "integer", "default": 2, "description": "Number of response times to return" },
          "response_times_average": { "type": "integer", "default": 0, "description": "Average response time in seconds" },
          "response_times_start_date": { "type": "integer", "description": "Start date (Unix timestamp)" },
          "response_times_end_date": { "type": "integer", "description": "End date (Unix timestamp)" },
          "alert_contacts": { "type": "integer", "enum": [0, 1], "default": 0, "description": "Include alert contacts: 0=no, 1=yes" },
          "mwindows": { "type": "integer", "enum": [0, 1], "default": 0, "description": "Include maintenance windows: 0=no, 1=yes" },
          "timezone": { "type": "integer", "default": 1, "description": "Timezone offset" },
          "offset": { "type": "integer", "default": 0, "description": "Offset for pagination" },
          "limit": { "type": "integer", "default": 50, "description": "Number of results" }
        },
        "required": ["api_key"]
      },
      "urlTemplate": "https://api.uptimerobot.com/v2/getMonitors"
    },
    {
      "id": "edit-monitor",
      "name": "Edit Monitor",
      "description": "Edit an existing monitor",
      "method": "POST",
      "path": "/editMonitor",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "api_key": { "type": "string", "required": true, "description": "API key" },
          "id": { "type": "string", "required": true, "description": "Monitor ID" },
          "friendly_name": { "type": "string", "description": "Monitor friendly name" },
          "url": { "type": "string", "format": "uri", "description": "URL to monitor" },
          "type": { "type": "integer", "enum": [1, 2, 3, 4], "description": "Monitor type" },
          "interval": { "type": "integer", "description": "Monitoring interval in seconds" },
          "keyword_type": { "type": "integer", "enum": [1, 2], "description": "Keyword type" },
          "keyword_value": { "type": "string", "description": "Keyword value" },
          "http_username": { "type": "string", "description": "HTTP basic auth username" },
          "http_password": { "type": "string", "description": "HTTP basic auth password" },
          "alert_contacts": { "type": "string", "description": "Comma-separated alert contact IDs" },
          "mwindows": { "type": "string", "description": "Comma-separated maintenance window IDs" },
          "status": { "type": "integer", "enum": [0, 1], "description": "Monitor status: 0=pause, 1=resume" }
        },
        "required": ["api_key", "id"]
      },
      "urlTemplate": "https://api.uptimerobot.com/v2/editMonitor"
    },
    {
      "id": "delete-monitor",
      "name": "Delete Monitor",
      "description": "Delete a monitor",
      "method": "POST",
      "path": "/deleteMonitor",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "api_key": { "type": "string", "required": true, "description": "API key" },
          "id": { "type": "string", "required": true, "description": "Monitor ID" }
        },
        "required": ["api_key", "id"]
      },
      "urlTemplate": "https://api.uptimerobot.com/v2/deleteMonitor"
    },
    {
      "id": "reset-monitor",
      "name": "Reset Monitor",
      "description": "Reset monitor statistics",
      "method": "POST",
      "path": "/resetMonitor",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "api_key": { "type": "string", "required": true, "description": "API key" },
          "id": { "type": "string", "required": true, "description": "Monitor ID" }
        },
        "required": ["api_key", "id"]
      },
      "urlTemplate": "https://api.uptimerobot.com/v2/resetMonitor"
    },
    {
      "id": "create-alert-contact",
      "name": "Create Alert Contact",
      "description": "Create a new alert contact",
