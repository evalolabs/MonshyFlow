          "value": { "type": "string", "required": true, "description": "Contact value (email, phone, webhook URL, etc.)" },
          "friendly_name": { "type": "string", "description": "Friendly name" }
        },
        "required": ["api_key", "type", "value"]
      },
      "urlTemplate": "https://api.uptimerobot.com/v2/newAlertContact"
    },
