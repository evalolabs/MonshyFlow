  "authentication": {
    "type": "apiKey",
    "headerName": "Authorization",
    "headerFormat": "{apiKey}",
    "secretKey": "CONTENTFUL_CONTENT_DELIVERY_ACCESS_TOKEN",
    "queryParamName": "access_token",
    "note": "Access token can be sent as query parameter (access_token) or in Authorization header. Preview API uses https://preview.contentful.com with CONTENTFUL_CONTENT_PREVIEW_ACCESS_TOKEN."
  },
  "endpoints": [
