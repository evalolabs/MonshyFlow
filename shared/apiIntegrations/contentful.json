      "id": "get-asset",
      "name": "Get Asset",
      "description": "Get asset details by ID",
      "method": "GET",
      "path": "/spaces/{spaceId}/assets/{assetId}",
      "headers": {},
      "queryParams": {
        "access_token": "{accessToken}"
      },
      "urlTemplate": "https://cdn.contentful.com/spaces/{spaceId}/assets/{assetId}"
    },
    {
      "id": "list-assets",
      "name": "List Assets",
      "description": "List all assets",
      "method": "GET",
      "path": "/spaces/{spaceId}/assets",
      "headers": {},
      "queryParams": {
        "access_token": "{accessToken}",
        "skip": "0",
        "limit": "100",
        "order": "sys.createdAt",
        "select": "{select}",
        "mimetype_group": "{mimetypeGroup}",
        "sys.id": "{sysId}",
        "sys.updatedAt": "{sysUpdatedAt}"
      },
      "urlTemplate": "https://cdn.contentful.com/spaces/{spaceId}/assets"
    },
