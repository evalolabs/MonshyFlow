          "mediaUrl": {
            "type": "string",
            "format": "uri",
            "description": "Media URL"
          },
          "assignedTo": {
            "type": "string",
            "description": "Assigned user ID"
          }
        },
        "required": ["content"]
      },
      "urlTemplate": "https://rest.gohighlevel.com/v1/conversations/{conversationId}/messages"
    },
    {
      "id": "list-messages",
      "name": "List Messages",
      "description": "List all messages in a conversation",
      "method": "GET",
      "path": "/conversations/{conversationId}/messages",
