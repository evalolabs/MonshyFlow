{
  "id": "highlevel",
  "name": "HighLevel (API)",
  "icon": "ðŸ“ˆ",
  "description": "All-in-one marketing platform for agencies",
  "baseUrl": "https://rest.gohighlevel.com/v1",
  "color": {
    "bg": "from-indigo-100 to-blue-100",
    "border": "border-indigo-500",
    "icon": "text-indigo-700",
    "handle": "bg-indigo-600"
  },
  "authentication": {
    "type": "apiKey",
    "headerName": "Authorization",
    "headerFormat": "Bearer {apiKey}",
    "secretKey": "HIGHLEVEL_API_KEY"
  },
  "endpoints": [
    {
      "id": "create-contact",
      "name": "Create Contact",
      "description": "Create a new contact",
      "method": "POST",
      "path": "/contacts",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "description": "First name"
          },
          "lastName": {
            "type": "string",
            "description": "Last name"
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "Email address"
          },
          "phone": {
            "type": "string",
            "description": "Phone number"
          },
          "companyName": {
            "type": "string",
            "description": "Company name"
          },
          "source": {
            "type": "string",
            "description": "Source"
          },
          "tags": {
            "type": "array",
            "items": { "type": "string" },
            "description": "Tags"
          },
          "customField": {
            "type": "object",
            "description": "Custom fields",
            "additionalProperties": true
          },
          "address1": { "type": "string" },
          "address2": { "type": "string" },
          "city": { "type": "string" },
          "state": { "type": "string" },
          "country": { "type": "string" },
          "postalCode": { "type": "string" },
          "website": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "urlTemplate": "https://rest.gohighlevel.com/v1/contacts"
    },
    {
      "id": "get-contact",
      "name": "Get Contact",
      "description": "Get contact by ID",
      "method": "GET",
      "path": "/contacts/{contactId}",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "urlTemplate": "https://rest.gohighlevel.com/v1/contacts/{contactId}"
    },
    {
      "id": "list-contacts",
      "name": "List Contacts",
      "description": "List all contacts",
      "method": "GET",
      "path": "/contacts",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "queryParams": {
        "limit": {
          "type": "integer",
          "default": 100,
          "description": "Maximum number of results"
        },
        "skip": {
          "type": "integer",
          "default": 0,
          "description": "Skip for pagination"
        },
        "query": {
          "type": "string",
          "description": "Search query"
        },
        "locationId": {
          "type": "string",
          "description": "Filter by location ID"
        },
        "tags": {
          "type": "string",
          "description": "Comma-separated tags"
        }
      },
      "urlTemplate": "https://rest.gohighlevel.com/v1/contacts"
    },
    {
      "id": "update-contact",
      "name": "Update Contact",
      "description": "Update a contact",
      "method": "PUT",
      "path": "/contacts/{contactId}",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "description": "Contact data to update",
        "additionalProperties": true
      },
      "urlTemplate": "https://rest.gohighlevel.com/v1/contacts/{contactId}"
    },
    {
      "id": "delete-contact",
      "name": "Delete Contact",
      "description": "Delete a contact",
      "method": "DELETE",
      "path": "/contacts/{contactId}",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "urlTemplate": "https://rest.gohighlevel.com/v1/contacts/{contactId}"
    },
