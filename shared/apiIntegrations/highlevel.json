      "id": "list-conversations",
      "name": "List Conversations",
      "description": "List all conversations",
      "method": "GET",
      "path": "/conversations",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "queryParams": {
        "limit": {
          "type": "integer",
          "default": 100
        },
        "skip": {
          "type": "integer",
          "default": 0
        },
        "locationId": {
          "type": "string",
          "description": "Filter by location ID"
        },
        "contactId": {
          "type": "string",
          "description": "Filter by contact ID"
        }
      },
      "urlTemplate": "https://rest.gohighlevel.com/v1/conversations"
    },
    {
      "id": "create-conversation",
      "name": "Create Conversation",
      "description": "Create a new conversation",
      "method": "POST",
      "path": "/conversations",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "contactId": {
            "type": "string",
            "required": true,
            "description": "Contact ID"
          },
          "phoneNumber": {
            "type": "string",
            "description": "Phone number"
          },
          "assignedTo": {
            "type": "string",
            "description": "Assigned user ID"
          },
          "status": {
            "type": "string",
            "enum": ["open", "closed"],
            "default": "open",
            "description": "Conversation status"
          }
        },
        "required": ["contactId"]
      },
      "urlTemplate": "https://rest.gohighlevel.com/v1/conversations"
    },
    {
      "id": "create-message",
      "name": "Create Message",
      "description": "Send a message in a conversation",
      "method": "POST",
      "path": "/conversations/{conversationId}/messages",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string",
