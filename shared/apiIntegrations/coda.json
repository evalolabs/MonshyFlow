      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "row": {
            "type": "object",
            "properties": {
              "cells": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "column": { "type": "string", "description": "Column ID or name" },
                    "value": { "type": ["string", "number", "boolean", "object"], "description": "Cell value" }
                  },
                  "required": ["column", "value"]
                },
                "description": "Array of cell objects"
              }
            },
            "required": ["cells"]
          },
          "disableParsing": { "type": "boolean", "default": false, "description": "Disable automatic parsing" }
        },
        "required": ["row"]
      },
