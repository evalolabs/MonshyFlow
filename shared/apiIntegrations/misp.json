  "description": "Open-source threat intelligence platform",
  "baseUrl": "https://{your-misp-instance}",
  "color": {
    "bg": "from-red-100 to-fuchsia-100",
    "border": "border-red-500",
    "icon": "text-red-700",
    "handle": "bg-red-600"
  },
  "authentication": {
    "type": "apiKey",
    "headerName": "Authorization",
    "headerFormat": "{apiKey}",
    "secretKey": "MISP_API_KEY",
    "note": "Base URL should be provided in baseUrl placeholder {your-misp-instance} or as MISP_BASE_URL secret key."
