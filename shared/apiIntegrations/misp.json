  "endpoints": [
    {
      "id": "create-event",
      "name": "Create Event",
      "description": "Create a new event",
      "method": "POST",
      "path": "/events/add",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "{apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "description": "Event data",
        "additionalProperties": true
      },
      "urlTemplate": "https://{your-misp-instance}/events/add"
    },
    {
      "id": "get-event",
      "name": "Get Event",
      "description": "Get event by ID",
      "method": "GET",
      "path": "/events/view/{eventId}",
      "headers": {
        "Authorization": "{apiKey}"
      },
      "urlTemplate": "https://{your-misp-instance}/events/view/{eventId}"
    },
    {
      "id": "list-events",
      "name": "List Events",
      "description": "List all events",
      "method": "GET",
      "path": "/events/index",
      "headers": {
        "Authorization": "{apiKey}"
      },
      "queryParams": {
        "limit": {
          "type": "integer",
          "description": "Maximum number of results"
        },
        "page": {
          "type": "integer",
          "description": "Page number"
        }
      },
      "urlTemplate": "https://{your-misp-instance}/events/index"
    },
