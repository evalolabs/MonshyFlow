      "method": "GET",
      "path": "/list/{listId}",
      "urlTemplate": "https://api.clickup.com/api/v2/list/{listId}"
    },
    {
      "id": "list-lists",
      "name": "List Lists",
      "description": "List all lists in a folder",
      "method": "GET",
      "path": "/folder/{folderId}/list",
      "queryParams": {
        "archived": "false"
      },
      "urlTemplate": "https://api.clickup.com/api/v2/folder/{folderId}/list"
    },
    {
      "id": "update-list",
      "name": "Update List",
      "description": "Update a list",
      "method": "PUT",
      "path": "/list/{listId}",
      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "name": { "type": "string", "description": "List name" },
          "content": { "type": "string", "description": "List description" },
          "due_date": { "type": "number", "description": "Due date timestamp" },
          "due_date_time": { "type": "boolean", "description": "Include time in due date" },
          "priority": { "type": "number", "enum": [1, 2, 3, 4] },
          "assignee": { "type": "string", "description": "Assignee user ID" },
          "status": { "type": "string", "description": "Status name" }
        }
      },
      "urlTemplate": "https://api.clickup.com/api/v2/list/{listId}"
    },
    {
      "id": "delete-list",
      "name": "Delete List",
      "description": "Delete a list",
      "method": "DELETE",
      "path": "/list/{listId}",
      "urlTemplate": "https://api.clickup.com/api/v2/list/{listId}"
    },
    {
