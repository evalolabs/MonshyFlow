          "due_date": { "type": "number", "description": "Due date timestamp" },
          "due_date_time": { "type": "boolean", "description": "Include time in due date", "default": false },
          "priority": { "type": "number", "enum": [1, 2, 3, 4], "description": "Priority" },
          "assignee": { "type": "string", "description": "Assignee user ID" },
          "status": { "type": "string", "description": "Status name" }
        },
        "required": ["name"]
