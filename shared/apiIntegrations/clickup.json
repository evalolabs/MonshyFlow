{
  "id": "clickup",
  "name": "ClickUp",
  "icon": "ðŸ“‹",
  "logoUrl": "https://cdn.simpleicons.org/clickup/7B68EE",
  "description": "Project management and productivity platform",
  "baseUrl": "https://api.clickup.com/api/v2",
  "color": {
    "bg": "from-violet-100 to-purple-100",
    "border": "border-violet-500",
    "icon": "text-violet-700",
    "handle": "bg-violet-600"
  },
  "authentication": {
    "type": "apiKey",
    "headerName": "Authorization",
    "headerFormat": "{apiKey}",
    "secretKey": "CLICKUP_API_KEY"
  },
  "endpoints": [
    {
      "id": "create-task",
      "name": "Create Task",
      "description": "Create a new task",
      "method": "POST",
      "path": "/list/{listId}/task",
      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "name": { "type": "string", "required": true, "description": "Task name" },
          "description": { "type": "string", "description": "Task description (Markdown)" },
          "assignees": {
            "type": "array",
            "items": { "type": "string" },
            "description": "Assignee user IDs"
          },
          "tags": {
            "type": "array",
            "items": { "type": "string" },
            "description": "Tag names"
          },
          "status": { "type": "string", "description": "Status name" },
          "priority": { "type": "number", "enum": [1, 2, 3, 4], "description": "Priority: 1=Urgent, 2=High, 3=Normal, 4=Low" },
          "due_date": { "type": "number", "description": "Due date timestamp (milliseconds)" },
          "due_date_time": { "type": "boolean", "description": "Include time in due date", "default": false },
          "time_estimate": { "type": "number", "description": "Time estimate in milliseconds" },
          "start_date": { "type": "number", "description": "Start date timestamp" },
          "start_date_time": { "type": "boolean", "description": "Include time in start date", "default": false },
          "notify_all": { "type": "boolean", "description": "Notify all assignees", "default": false },
          "parent": { "type": "string", "description": "Parent task ID" },
          "links_to": { "type": "string", "description": "Link to task ID" }
        },
        "required": ["name"]
      },
      "urlTemplate": "https://api.clickup.com/api/v2/list/{listId}/task"
    },
    {
      "id": "get-task",
      "name": "Get Task",
      "description": "Get task details by ID",
      "method": "GET",
      "path": "/task/{taskId}",
      "queryParams": {
        "custom_task_ids": "false",
        "team_id": "{teamId}",
        "include_subtasks": "false"
      },
      "urlTemplate": "https://api.clickup.com/api/v2/task/{taskId}"
    },
    {
      "id": "list-tasks",
      "name": "List Tasks",
      "description": "List tasks",
      "method": "GET",
      "path": "/list/{listId}/task",
      "queryParams": {
        "archived": "false",
        "page": "0",
        "order_by": "created",
        "reverse": "false",
        "subtasks": "false",
        "statuses[]": "{statuses}",
        "include_closed": "false",
        "assignees[]": "{assignees}",
        "tags[]": "{tags}",
        "due_date_gt": "{timestamp}",
        "due_date_lt": "{timestamp}",
        "date_created_gt": "{timestamp}",
        "date_created_lt": "{timestamp}",
        "date_updated_gt": "{timestamp}",
        "date_updated_lt": "{timestamp}"
      },
      "urlTemplate": "https://api.clickup.com/api/v2/list/{listId}/task"
    },
    {
      "id": "update-task",
      "name": "Update Task",
      "description": "Update an existing task",
      "method": "PUT",
      "path": "/task/{taskId}",
      "headers": {
        "Content-Type": "application/json"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "name": { "type": "string", "description": "Task name" },
          "description": { "type": "string", "description": "Task description (Markdown)" },
          "status": { "type": "string", "description": "Status name" },
          "priority": { "type": "number", "enum": [1, 2, 3, 4] },
          "due_date": { "type": "number", "description": "Due date timestamp (milliseconds)" },
          "due_date_time": { "type": "boolean", "description": "Include time in due date" },
          "parent": { "type": "string", "description": "Parent task ID" },
          "time_estimate": { "type": "number", "description": "Time estimate in milliseconds" },
          "start_date": { "type": "number", "description": "Start date timestamp" },
          "start_date_time": { "type": "boolean", "description": "Include time in start date" },
          "assignees": {
            "type": "object",
            "properties": {
              "add": {
                "type": "array",
                "items": { "type": "string" }
              },
              "rem": {
                "type": "array",
                "items": { "type": "string" }
              }
            },
            "description": "Add or remove assignees"
          }
        }
      },
      "urlTemplate": "https://api.clickup.com/api/v2/task/{taskId}"
    },
    {
      "id": "delete-task",
      "name": "Delete Task",
      "description": "Delete a task",
      "method": "DELETE",
      "path": "/task/{taskId}",
      "queryParams": {
        "custom_task_ids": "false",
        "team_id": "{teamId}"
      },
      "urlTemplate": "https://api.clickup.com/api/v2/task/{taskId}"
    },
    {
      "id": "create-list",
