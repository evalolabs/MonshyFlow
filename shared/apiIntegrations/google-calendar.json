{
  "id": "google-calendar",
  "name": "Google Calendar",
  "icon": "ðŸ“…",
  "logoUrl": "https://cdn.simpleicons.org/googlecalendar/4285F4",
  "description": "Calendar and event management",
  "baseUrl": "https://www.googleapis.com/calendar/v3",
  "color": {
    "bg": "from-blue-100 to-indigo-100",
    "border": "border-blue-500",
    "icon": "text-blue-700",
    "handle": "bg-blue-600"
  },
  "authentication": {
    "type": "bearer",
    "headerName": "Authorization",
    "headerFormat": "Bearer {apiKey}",
    "secretKey": "GOOGLE_CALENDAR_ACCESS_TOKEN"
  },
  "endpoints": [
    {
      "id": "create-calendar",
      "name": "Create Calendar",
      "description": "Create a new calendar",
      "method": "POST",
      "path": "/calendars",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "summary": { "type": "string", "required": true, "description": "Calendar summary (name)" },
          "description": { "type": "string", "description": "Calendar description" },
          "location": { "type": "string", "description": "Calendar location" },
          "timeZone": { "type": "string", "description": "Time zone (e.g., America/New_York)" }
        },
        "required": ["summary"]
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars"
    },
    {
      "id": "get-calendar",
      "name": "Get Calendar",
      "description": "Get calendar by ID",
      "method": "GET",
      "path": "/calendars/{calendarId}",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars/{calendarId}"
    },
    {
      "id": "list-calendars",
      "name": "List Calendars",
      "description": "List all calendars",
      "method": "GET",
      "path": "/users/me/calendarList",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "queryParams": {
        "minAccessRole": { "type": "string", "enum": ["freeBusyReader", "reader", "writer", "owner"], "description": "Minimum access role" },
        "pageToken": { "type": "string", "description": "Token for pagination" },
        "showDeleted": { "type": "boolean", "default": false, "description": "Show deleted calendars" },
        "showHidden": { "type": "boolean", "default": false, "description": "Show hidden calendars" },
        "maxResults": { "type": "integer", "default": 250, "description": "Maximum number of results" }
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/users/me/calendarList"
    },
    {
      "id": "update-calendar",
      "name": "Update Calendar",
      "description": "Update a calendar",
      "method": "PUT",
      "path": "/calendars/{calendarId}",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "summary": { "type": "string", "description": "Calendar summary (name)" },
          "description": { "type": "string", "description": "Calendar description" },
          "location": { "type": "string", "description": "Calendar location" },
          "timeZone": { "type": "string", "description": "Time zone (e.g., America/New_York)" }
        }
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars/{calendarId}"
    },
    {
      "id": "delete-calendar",
      "name": "Delete Calendar",
      "description": "Delete a calendar",
      "method": "DELETE",
      "path": "/calendars/{calendarId}",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars/{calendarId}"
    },
    {
      "id": "create-event",
      "name": "Create Event",
      "description": "Create a new event",
      "method": "POST",
      "path": "/calendars/{calendarId}/events",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "summary": { "type": "string", "required": true, "description": "Event summary (title)" },
          "description": { "type": "string", "description": "Event description" },
          "location": { "type": "string", "description": "Event location" },
          "start": {
            "type": "object",
            "properties": {
              "dateTime": { "type": "string", "format": "date-time", "description": "Start date and time (ISO 8601)" },
              "date": { "type": "string", "format": "date", "description": "Start date (all-day event)" },
              "timeZone": { "type": "string", "description": "Time zone" }
            },
            "required": true
          },
          "end": {
            "type": "object",
            "properties": {
              "dateTime": { "type": "string", "format": "date-time", "description": "End date and time (ISO 8601)" },
              "date": { "type": "string", "format": "date", "description": "End date (all-day event)" },
              "timeZone": { "type": "string", "description": "Time zone" }
            },
            "required": true
          },
          "recurrence": {
            "type": "array",
            "items": { "type": "string" },
            "description": "Recurrence rules (RRULE format)"
          },
          "attendees": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "email": { "type": "string", "format": "email", "required": true, "description": "Attendee email" },
                "displayName": { "type": "string", "description": "Display name" },
                "optional": { "type": "boolean", "default": false, "description": "Optional attendee" },
                "responseStatus": { "type": "string", "enum": ["needsAction", "declined", "tentative", "accepted"], "description": "Response status" }
              },
              "required": ["email"]
            },
            "description": "Event attendees"
          },
          "reminders": {
            "type": "object",
            "properties": {
              "useDefault": { "type": "boolean", "default": true, "description": "Use default reminders" },
              "overrides": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "method": { "type": "string", "enum": ["email", "popup"], "required": true, "description": "Reminder method" },
                    "minutes": { "type": "integer", "required": true, "description": "Minutes before event" }
                  },
                  "required": ["method", "minutes"]
                }
              }
            }
          },
          "colorId": { "type": "string", "description": "Color ID" },
          "visibility": { "type": "string", "enum": ["default", "public", "private", "confidential"], "default": "default", "description": "Event visibility" },
          "transparency": { "type": "string", "enum": ["opaque", "transparent"], "default": "opaque", "description": "Event transparency" },
          "guestsCanModify": { "type": "boolean", "default": false, "description": "Guests can modify" },
          "guestsCanInviteOthers": { "type": "boolean", "default": true, "description": "Guests can invite others" },
          "guestsCanSeeOtherGuests": { "type": "boolean", "default": true, "description": "Guests can see other guests" }
        },
        "required": ["summary", "start", "end"]
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars/{calendarId}/events"
    },
    {
      "id": "get-event",
      "name": "Get Event",
      "description": "Get event by ID",
      "method": "GET",
      "path": "/calendars/{calendarId}/events/{eventId}",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "queryParams": {
        "timeZone": { "type": "string", "description": "Time zone" },
        "maxAttendees": { "type": "integer", "description": "Maximum number of attendees" },
        "alwaysIncludeEmail": { "type": "boolean", "default": false, "description": "Always include email" }
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars/{calendarId}/events/{eventId}"
    },
    {
      "id": "list-events",
      "name": "List Events",
      "description": "List all events in a calendar",
      "method": "GET",
      "path": "/calendars/{calendarId}/events",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "queryParams": {
        "timeMin": { "type": "string", "format": "date-time", "description": "Minimum time (ISO 8601)" },
        "timeMax": { "type": "string", "format": "date-time", "description": "Maximum time (ISO 8601)" },
        "timeZone": { "type": "string", "description": "Time zone" },
        "maxResults": { "type": "integer", "default": 250, "description": "Maximum number of results" },
        "pageToken": { "type": "string", "description": "Token for pagination" },
        "singleEvents": { "type": "boolean", "default": false, "description": "Expand recurring events" },
        "orderBy": { "type": "string", "enum": ["startTime", "updated"], "default": "startTime", "description": "Sort order" },
        "q": { "type": "string", "description": "Search query" },
        "iCalUID": { "type": "string", "description": "iCal UID" },
        "showDeleted": { "type": "boolean", "default": false, "description": "Show deleted events" },
        "showHiddenInvitations": { "type": "boolean", "default": false, "description": "Show hidden invitations" },
        "updatedMin": { "type": "string", "format": "date-time", "description": "Minimum update time" },
        "maxAttendees": { "type": "integer", "description": "Maximum number of attendees" },
        "alwaysIncludeEmail": { "type": "boolean", "default": false, "description": "Always include email" }
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars/{calendarId}/events"
    },
    {
      "id": "update-event",
      "name": "Update Event",
      "description": "Update an existing event",
      "method": "PUT",
      "path": "/calendars/{calendarId}/events/{eventId}",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {apiKey}"
      },
      "queryParams": {
        "sendUpdates": { "type": "string", "enum": ["all", "externalOnly", "none"], "default": "all", "description": "Send update notifications" },
        "conferenceDataVersion": { "type": "integer", "default": 0, "description": "Conference data version" }
      },
      "bodySchema": {
        "type": "object",
        "properties": {
          "summary": { "type": "string", "description": "Event summary (title)" },
          "description": { "type": "string", "description": "Event description" },
          "location": { "type": "string", "description": "Event location" },
          "start": {
            "type": "object",
            "properties": {
              "dateTime": { "type": "string", "format": "date-time", "description": "Start date and time (ISO 8601)" },
              "date": { "type": "string", "format": "date", "description": "Start date (all-day event)" },
              "timeZone": { "type": "string", "description": "Time zone" }
            }
          },
          "end": {
            "type": "object",
            "properties": {
              "dateTime": { "type": "string", "format": "date-time", "description": "End date and time (ISO 8601)" },
              "date": { "type": "string", "format": "date", "description": "End date (all-day event)" },
              "timeZone": { "type": "string", "description": "Time zone" }
            }
          },
          "recurrence": {
            "type": "array",
            "items": { "type": "string" }
          },
          "attendees": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "email": { "type": "string", "format": "email", "description": "Attendee email" },
                "displayName": { "type": "string", "description": "Display name" },
                "optional": { "type": "boolean", "description": "Optional attendee" },
                "responseStatus": { "type": "string", "enum": ["needsAction", "declined", "tentative", "accepted"], "description": "Response status" }
              }
            }
          },
          "reminders": {
            "type": "object",
            "properties": {
              "useDefault": { "type": "boolean", "description": "Use default reminders" },
              "overrides": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "method": { "type": "string", "enum": ["email", "popup"], "description": "Reminder method" },
                    "minutes": { "type": "integer", "description": "Minutes before event" }
                  }
                }
              }
            }
          },
          "colorId": { "type": "string", "description": "Color ID" },
          "visibility": { "type": "string", "enum": ["default", "public", "private", "confidential"], "description": "Event visibility" },
          "transparency": { "type": "string", "enum": ["opaque", "transparent"], "description": "Event transparency" },
          "guestsCanModify": { "type": "boolean", "description": "Guests can modify" },
          "guestsCanInviteOthers": { "type": "boolean", "description": "Guests can invite others" },
          "guestsCanSeeOtherGuests": { "type": "boolean", "description": "Guests can see other guests" }
        }
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars/{calendarId}/events/{eventId}"
    },
    {
      "id": "delete-event",
      "name": "Delete Event",
      "description": "Delete an event",
      "method": "DELETE",
      "path": "/calendars/{calendarId}/events/{eventId}",
      "headers": {
        "Authorization": "Bearer {apiKey}"
      },
      "queryParams": {
        "sendUpdates": { "type": "string", "enum": ["all", "externalOnly", "none"], "default": "none", "description": "Send update notifications" }
      },
      "urlTemplate": "https://www.googleapis.com/calendar/v3/calendars/{calendarId}/events/{eventId}"
    }
  ]
}

